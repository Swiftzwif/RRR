{
  "metadata": {
    "title": "Lane Diagnostic Quiz",
    "description": "Discover which financial lane you're actually in based on MJ DeMarco's Millionaire Fastlane framework",
    "lanes": ["sidewalk", "slowlane", "fastlane"],
    "categories": ["financial_mindset", "time_freedom", "risk_opportunity", "systems_scalability"],
    "scale": {
      "min": 1,
      "max": 5,
      "labels": {
        "1": "Strongly Disagree", 
        "2": "Disagree",
        "3": "Neutral", 
        "4": "Agree",
        "5": "Strongly Agree"
      }
    },
    "laneDescriptions": {
      "sidewalk": {
        "name": "Sidewalk Lane",
        "description": "Living paycheck to paycheck, no financial plan, consumer mindset",
        "characteristics": [
          "Spends more than earns",
          "No emergency fund",
          "Consumer-focused mindset",
          "No long-term financial plan"
        ]
      },
      "slowlane": {
        "name": "Slowlane",
        "description": "Traditional path (job → save → invest → retire at 65), linear income",
        "characteristics": [
          "Trades time for money",
          "Linear income growth",
          "Traditional retirement planning",
          "Security over growth"
        ]
      },
      "fastlane": {
        "name": "Fastlane",
        "description": "Entrepreneurial path, building systems/assets, exponential income potential",
        "characteristics": [
          "Builds systems and assets",
          "Exponential income potential",
          "Leverage and multiplication",
          "Freedom and control"
        ]
      }
    }
  },
  "questions": [
    {
      "id": "LD1",
      "category": "financial_mindset",
      "prompt": "When I receive a bonus or windfall, my first instinct is to...",
      "type": "multiple_choice",
      "options": [
        { "value": 1, "text": "Buy something I've been wanting" },
        { "value": 2, "text": "Pay off some debt" },
        { "value": 3, "text": "Put it in savings" },
        { "value": 4, "text": "Invest it in stocks or bonds" },
        { "value": 5, "text": "Invest it in a business or income-producing asset" }
      ]
    },
    {
