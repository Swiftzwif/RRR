{
  "metadata": {
    "title": "Lane Diagnostic Game",
    "description": "Gamified assessment to discover your financial lane with timers, insights, and anti-gaming measures",
    "lanes": ["sidewalk", "slowlane", "fastlane"],
    "categories": ["financial_mindset", "time_freedom", "risk_opportunity", "systems_scalability"],
    "scale": {
      "min": 1,
      "max": 5,
      "labels": {
        "1": "Strongly Disagree", 
        "2": "Disagree",
        "3": "Neutral", 
        "4": "Agree",
        "5": "Strongly Agree"
      }
    },
    "gameSettings": {
      "timerSeconds": 60,
      "totalQuestions": 18,
      "validationQuestions": 2,
      "milestoneThresholds": [0.25, 0.5, 0.75, 1.0],
      "microInsightInterval": 3
    },
    "milestoneMessages": {
      "25": "Decisive starts—trust your first instinct.",
      "50": "Half done. Momentum beats perfection.",
      "75": "Strong pace—finish like a closer.",
      "100": "Ownership unlocked."
    },
    "microInsights": {
      "financial_mindset": {
        "high": "You value autonomy—convert it into assets, not hours.",
        "low": "Consumer mindset detected. Start with one asset purchase this month."
      },
      "time_freedom": {
        "high": "Time freedom is your priority. Build systems that work without you.",
        "low": "Your calendar owns you. One ruthless rule frees 5 hours this week."
      },
      "risk_opportunity": {
        "high": "Opportunity-oriented—aim it at one repeatable play.",
        "low": "Risk-averse patterns. Start with one small calculated risk this month."
      },
      "systems_scalability": {
        "high": "Systems thinking strong. Focus on one scalable process this quarter.",
        "low": "No systems, no scale. Start a 3-step daily pipeline."
      }
    },
    "badges": {
      "SKY_PILOT": {
        "name": "Sky Pilot",
        "description": "Completed the Lane Diagnostic",
        "condition": "completed_once"
      },
      "FIRST_INSTINCT": {
        "name": "First Instinct",
        "description": "Fast average response time with minimal changes",
        "condition": "fast_consistent"
      },
      "STEADY_CLIMB": {
        "name": "Steady Climb",
        "description": "Zero timeouts during assessment",
        "condition": "no_timeouts"
      },
      "HONEST_SIGNAL": {
        "name": "Honest Signal",
        "description": "High consistency in validation questions",
        "condition": "high_consistency"
      },
      "CLOSER_90": {
        "name": "Closer 90",
        "description": "Strong finish with high scores in final questions",
        "condition": "strong_finish"
      }
    }
  },
