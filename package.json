{
  "name": "trajectory-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cd apps/main && npm run dev",
    "build": "cd apps/main && npm run build",
    "lint": "cd apps/main && npm run lint",
    "typecheck": "cd apps/main && npm run typecheck",
    "install:all": "npm install && cd apps/main && npm install"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20.19.11"
  },
  "packageManager": "npm@10.0.0"
}
